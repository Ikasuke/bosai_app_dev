<!DOCTYPE html>
<html>
  <head>
    <title>BosaiAppDev</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body data-controller="<%= params[:controller] %>" data-action="<%= params[:action] %>" >
   <%= render "layouts/header" %>
    <div class="container py-2 px-0">
        <%= flash_message(flash[:success], :success) if flash[:success] %>
        <%= flash_message(flash[:error], :danger) if flash[:error] %>
        <%= flash_message(flash[:alert], :warning) if flash[:alert] %>
        <%= flash_message(flash[:notice], :info) if flash[:notice] %>
        <div class="row">
              <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12"> <%= yield :sidebar_left %></div>
              
              <div class="col-lg-9 col-md-9 col-sm-12 col-xs-12"><%= yield %> </div>
        </div>
  </div>
  </body>
</html>
